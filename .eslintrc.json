{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "consistent-return": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "space-before-function-paren": 0,
    "import/no-extraneous-dependencies": 0,
    "jsx-a11y/href-no-hash": [0],
    "react/forbid-foreign-prop-types": 1
  }
}
