{
  "name": "@bufferapp/login",
  "version": "0.0.1",
  "description": "A buffer login form",
  "main": "index.js",
  "scripts": {
    "start": "start-storybook -p 9003",
    "lint": "eslint . --ignore-pattern coverage .storybook node_modules",
    "test": "npm run lint && jest --coverage",
    "test-watch": "jest --watch",
    "test-update": "jest -u"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@bufferapp/components)"
    ],
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "keywords": [
    "login",
    "component"
  ],
  "author": "hharnis@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@bufferapp/components": "0.4.16",
    "deep-freeze": "0.0.1",
    "micro-rpc-client": "0.0.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.3",
    "redux-form": "6.6.3"
  },
  "devDependencies": {
    "@kadira/storybook": "2.35.3",
    "babel-eslint": "7.2.1",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "0.28.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.7.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "19.0.2",
    "postcss-custom-properties": "5.0.2",
    "postcss-hexrgba": "0.2.1",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.3",
    "react-addons-test-utils": "^15.4.2",
    "storyshots": "3.2.2",
    "style-loader": "0.16.1"
  }
}
