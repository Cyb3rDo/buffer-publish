{
  "name": "@bufferapp/server-publish",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "micro",
    "lint": "eslint . --ignore-pattern coverage node_modules",
    "test": "npm run lint && sh ../../package_test.sh",
    "test-watch": "jest --watch"
  },
  "jest": {
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@bufferapp/*)"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "author": "hharnisc@gmail.com",
  "dependencies": {
    "@bufferapp/micro-rpc": "0.1.1",
    "micro": "7.3.3",
    "micro-rpc-client": "0.0.1",
    "microrouter": "2.1.1",
    "request": "2.81.0",
    "request-promise": "4.2.1"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "jest": "19.0.2"
  }
}
