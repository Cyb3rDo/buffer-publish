{
  "name": "@bufferapp/publish-routes",
  "version": "0.0.1",
  "description": "Keep track of and generate buffer routes",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ignore-pattern coverage node_modules",
    "test": "npm run lint && sh ../../package_test.sh",
    "test-watch": "jest --watch"
  },
  "jest": {
    "verbose": true,
    "transformIgnorePatterns": [
      "node_modules/(?!@bufferapp/*)"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "author": "hharnisc@gmail.com",
  "devDependencies": {
    "eslint": "3.19.0",
    "jest": "19.0.2"
  }
}
