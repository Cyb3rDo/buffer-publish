{
  "name": "@bufferapp/web",
  "version": "0.0.1",
  "description": "Buffer Web Dashboard",
  "main": "index.jsx",
  "scripts": {
    "start": "start-storybook -p 9003",
    "lint": "eslint . --ignore-pattern coverage .storybook node_modules",
    "test": "npm run lint && sh ../../package_test.sh",
    "test-watch": "jest --watch",
    "test-update": "jest -u"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@bufferapp/*)"
    ],
    "verbose": true
  },
  "dependencies": {
    "@bufferapp/components": "0.5.22",
    "@bufferapp/store": "0.0.1",
    "@bufferapp/example": "0.0.1",
    "@bufferapp/tabs": "0.0.1",
    "@bufferapp/async-data-fetch": "0.0.1"
  },
  "author": "Harrison Harnisch <hharnisc@gmail.com> (http://hharnisc.github.io)",
  "devDependencies": {
    "@storybook/addon-storyshots": "3.1.4",
    "@storybook/react": "3.1.3",
    "eslint": "3.19.0",
    "jest": "20.0.4",
    "storybook-addon-a11y": "3.0.0"
  }
}
